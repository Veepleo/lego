{
  "name": "lego-sql",
  "version": "3.0.0",
  "description": "A SQL (string) builder using ES6 template strings. Lego embraces SQL instead of adding yet another abstraction layer. It automatically generates your migrations based on your evolving schema.",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "build": "babel src -d lib",
    "coverage": "istanbul cover _mocha -- --compilers js:babel-register --recursive ./test/*.js",
    "test": "NODE_ENV=test mocha --compilers js:babel-register --require babel-polyfill --reporter spec ./test/*.js",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "bench": "node bench/simple.js"
  },
  "engines": {
    "node": ">=7.3.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martijndeh/lego.git"
  },
  "author": "Martijn de Haan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/martijndeh/lego/issues"
  },
  "bin": {
    "lego": "lib/bin/script.js"
  },
  "homepage": "https://github.com/martijndeh/lego#readme",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-register": "^6.24.0",
    "blanket": "^1.2.1",
    "coveralls": "^2.11.6",
    "eslint": "^3.8.0",
    "istanbul": "^1.0.0-alpha.2",
    "knex": "^0.12.7",
    "microtime": "^2.1.2",
    "mocha": "^2.3.4",
    "mocha-lcov-reporter": "^1.0.0",
    "sinon": "^1.17.2"
  },
  "keywords": [
    "lego",
    "sql",
    "postgres",
    "knex",
    "sql builder",
    "template strings"
  ],
  "dependencies": {
    "autobind-decorator": "^1.3.4",
    "chalk": "^1.1.1",
    "debug": "^2.2.0",
    "dotenv": "^1.2.0",
    "minimist": "^1.2.0",
    "pg": "^6.1.0",
    "pg-query-parser": "^0.2.1"
  }
}
